{
  "name": "series.andersos.net",
  "description": "An overview of the series I watch",
  "version": "0.0.1",
  "homepage": "series.andersos.net",
  "repository": {
    "type": "git",
    "url": "git@github.com:Andersos/series.andersos.net.git"
  },
  "scripts": {
    "build": "webpack --config webpack-production.config.js --progress --colors",
    "deploy": "npm run build && git subtree push --prefix build origin gh-pages",
    "dev": "npm run start",
    "start": "webpack-dev-server --config webpack-dev-server.config.js --progress --inline --colors",
    "tdd": "eslint . && ./node_modules/.bin/mocha --compilers js:babel-core/register --watch",
    "test": "eslint . && ./node_modules/.bin/mocha --compilers js:babel-core/register"
  },
  "devDependencies": {
    "babel-core": "^6.3.26",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "eslint": "^2.5.1",
    "eslint-config-andersos": "^0.2.0",
    "eslint-plugin-react": "^4.0.0",
    "html-webpack-plugin": "^2.7.2",
    "react-hot-loader": "^1.3.0",
    "transfer-webpack-plugin": "^0.1.4",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "exports-loader": "^0.6.3",
    "imports-loader": "^0.6.5",
    "material-ui": "^0.15.0-beta.2",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-tap-event-plugin": "^1.0.0",
    "whatwg-fetch": "^1.0.0"
  }
}
